{
  "name": "@toolz/material-ui",
  "version": "2.0.11",
  "description": "A few simple (borderline-silly, they're so simple) wrapper components for Material UI Grid components",
  "files": [
    "dist"
  ],
  "keywords": [
    "Material UI",
    "React",
    "javascript",
    "responsive",
    "row",
    "column",
    "grid",
    "layout"
  ],
  "homepage": "https://github.com/bytebodger/material-ui",
  "bugs": {
    "email": "me@adamdavis.codes",
    "url": "https://github.com/bytebodger/material-ui/issues"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.3.1",
    "@mui/material": "^5.4.0",
    "@mui/styled-engine-sc": "^5.3.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-build-dist": "0.0.8",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "del-cli --force dist && set NODE_ENV=production && babel src/lib --out-dir dist --copy-files",
    "build_orig": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:dist": "react-build-dist"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bytebodger/material-ui.git"
  },
  "author": "Adam Nathaniel Davis <me@adamdavis.codes> (https://adamdavis.codes)",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "del-cli": "^4.0.1"
  }
}
